<h4>Upcoming Events</h4>

<table class="table table-hover">
  <thead>
  <th>N</th>
  <th>Event</th>
  <th>Location</th>
  <th>Date</th>
  </thead>
  <tbody>
  <% @events.each_with_index do |event, i| %>
    <tr>
      <td><%= i+1 %></td>
      <td><%= link_to event.name, event %></td>
      <td><%= event.location %></td>
      <td><%= event.voted_date %></td>
    </tr>
  <% end %>
  </tbody>
</table>