<div class="row ml-0">
  <%= form_with(model: date_option, local: true) do |form| %>
    <div class="form-group">
      <%#= form.label "Add a Date Option for this event:" %>
      <div class="form-group">
        <%= form.label :date %>
        <%= form.datetime_local_field :date , required: true%>
      </div>
      <div class="form-group">
        <%#= form.label :event_id %>
        <%= form.hidden_field :event_id,value: event, class: "form-control"%>
      </div>
    </div>


    <div class="actions">
      <%= form.submit %>
    </div>
  <% end %>
</div>
