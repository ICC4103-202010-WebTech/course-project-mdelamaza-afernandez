<div class="row ml-0">
<%= form_with(model: date_option, local: true) do |form| %>
    <div class="form-group">
      <%#= form.label "Add a Date Option for this event:" %>
      <div class="form-group">
        <%= form.label :date %>
        <%= form.datetime_local_field :date %>
      </div>
      <div class="form-group">
        <%= form.label :event_id %>
        <%= form.collection_select :event_id , Event.where(id: event),:id,:name %>
      </div>
    </div>


  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
</div>
