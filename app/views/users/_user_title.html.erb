<div class="col-md-4">
  <div class="card mb-4 shadow-sm">
    <div>
      <%if user.flyer.attached?%>
        <%= link_to image_tag(user.flyer, width: "400px"), url_for(user) %>
      <%else%>
        <%= link_to image_tag("thumbnail.svg", class: "card-img-top", width: "400px", background: "#55595c", color: "#eceeef"), url_for(user) %>
      <%end%>
    </div>
    <div class="card-body">
      <p class="card-text">
        <%= "Name:" %>
        <%= link_to user.user_name, user %></p>
      <p class="card-text">
        <%= "Description:" %>
        <%= user.description %></p>
      <!--      <p class="card-text"><%#= organization.voted_date %></p>-->
      <!--  <div class="d-flex justify-content-between align-items-center">-->
      <div class="btn-group">
        <button type="button" class="btn btn-sm btn-outline-secondary"><%= link_to "View", user %></button>
        <% if owner or admin  %>
                <button type="button" class="btn btn-sm btn-outline-secondary"><%= link_to "Edit", edit_user_path(user) %></button>
          <% end %>
      </div>
      <!--    <small class="text-muted"><%#= "#{(event.voted_date - DateTime.now.to_date).to_i} To Vote!" %></small>-->
      <!--  </div>-->
    </div>
  </div>
</div>